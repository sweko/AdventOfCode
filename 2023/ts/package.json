{
  "name": "advent-of-code-2023",
  "version": "1.0.0",
  "description": "Advent of Code solutions for 2023",
  "main": "index.js",
  "scripts": {
    "setup-day": "ts-node ./system/setup-day.ts",
    "start": "ts-node ./system/run-task.ts",
    "start:debug": "set DEBUG=true && ts-node ./system/run-task.ts",
    "start:extra": "node --stack-size=19600 -r ts-node/register ./system/run-task.ts",
    "all": "ts-node ./system/run-all.ts",
    "test": "set TEST=true && ts-node ./system/run-task.ts"
  },
  "author": "Wekoslav Stefanovski",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@types/node": "^20.10.0",
    "terminal-kit": "^3.0.1"
  }
}
